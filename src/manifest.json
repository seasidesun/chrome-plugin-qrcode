{
  "manifest_version": 2,
  "name": "qrcode",
  "description": "A popups tool to generate qrcode",
  "version": "1.0",
  "icons": {
    "128": "img/icon-128.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popups/popup.html",
    "default_icon": "img/icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
